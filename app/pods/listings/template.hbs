<article class="clearfix" data-template="listings">
  <section class="clearfix margin-bottom-10">
    <h1>{{t "listings"}}</h1>
  </section>
  <section class="clearfix">
    <div class="clearfix margin-bottom-10 padding-bottom-10">
      <div><input type="text" value={{model.delegate.filters.title}} oninput={{action "search"}} /></div>
    </div>
    <div class="clearfix">{{listing-table delegate=model.delegate}}</div>
  </section>
</article>
